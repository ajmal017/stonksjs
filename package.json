{
  "name": "stonksjs",
  "version": "1.0.0",
  "description": "Algotrading tools in node",
  "main": "lib/Stonks.js",
  "files": [
    "lib/",
    "config/",
    "index.js"
  ],
  "directories": {
    "lib": "lib/",
    "doc": "README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nielse63/stonksjs.git"
  },
  "homepage": "https://github.com/nielse63/stonksjs#readme",
  "bugs": {
    "url": "https://github.com/nielse63/stonksjs/issues"
  },
  "keywords": [
    "alpaca",
    "algotrading",
    "stocks"
  ],
  "engines": {
    "node": "^13.12.0",
    "npm": "^6.14.4"
  },
  "scripts": {
    "start": "node index.js",
    "lint": "eslint . --fix",
    "test": "jest --coverage",
    "release": "dotenv release-it",
    "prepublish": "npm run build",
    "build": "babel src --out-dir lib --source-maps inline --ignore \"**/__mocks__,**/__tests__\" --delete-dir-on-start"
  },
  "author": {
    "name": "Erik Nielsen",
    "email": "erik@312development.com",
    "url": "https://github.com/nielse63/"
  },
  "license": "ISC",
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^1.2.9",
    "camelcase-keys": "^6.2.2",
    "chalk": "^4.0.0",
    "cheerio": "^1.0.0-rc.3",
    "date-fns": "^2.11.1",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.15",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "technicalindicators": "^3.1.0",
    "unirest": "^0.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-flow": "^7.9.0",
    "@release-it/conventional-changelog": "^1.1.4",
    "auto-changelog": "^2.0.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^25.5.1",
    "depcheck": "^0.9.2",
    "dotenv-cli": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "flow-bin": "^0.123.0",
    "husky": "^4.2.5",
    "jest": "^25.5.3",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "release-it": "^13.5.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "release-it": {
    "github": {
      "release": true
    }
  }
